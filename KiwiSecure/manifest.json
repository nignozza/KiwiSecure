{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "declarativeContent", "storage", "experimental"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "KiwiLogo128.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "127": "images/get_started128.png"
    }
  },
  "icons": {
    "128": "KiwiLogo128.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "127": "images/get_started128.png"
  },
  "manifest_version": 2
}
